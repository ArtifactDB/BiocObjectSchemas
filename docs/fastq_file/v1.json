{
    "$id": "fastq_file/v1.json",
    "$schema": "http://json-schema.org/draft-07/schema",
    "additionalProperties": false,
    "description": "A FASTQ file containing a collection of biological sequences with quality scores, typically corresponding to experimental data.",
    "properties": {
        "$schema": {
            "description": "The schema to use.",
            "type": "string"
        },
        "fastq_file": {
            "properties": {
                "compression": {
                    "description": "The compression format.",
                    "enum": [
                        "none",
                        "gzip",
                        "bzip2",
                        "bgzip"
                    ],
                    "type": "string"
                },
                "quality_encoding": {
                    "default": "phred",
                    "description": "Encoding used for the base quality scores in a FASTQ file.",
                    "enum": [
                        "phred",
                        "solexa",
                        "illumina"
                    ],
                    "type": "string"
                },
                "type": {
                    "default": "DNA",
                    "description": "Type of sequence in the file.",
                    "enum": [
                        "DNA",
                        "RNA",
                        "AA"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "is_child": {
            "default": false,
            "description": "Is this a child document, only to be interpreted in the context of the parent document from which it is linked? This may have implications for search and metadata requirements.",
            "type": "boolean"
        },
        "md5sum": {
            "description": "MD5 checksum for the file.",
            "type": "string"
        },
        "path": {
            "description": "Path to the file in the project directory.",
            "type": "string"
        }
    },
    "required": [
        "$schema",
        "fastq_file",
        "md5sum",
        "path"
    ],
    "title": "FASTQ File",
    "type": "object"
}
